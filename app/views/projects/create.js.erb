<% if @project_exists %>
  swal({
      icon: "error",
      title: 'Oops..!',
      buttons: false,
      timer: 3000,
      text: "<%= t('views.projects.duplicate_name') %>"
  });
<% else %>
  swal({
      icon: "success",
      title: 'Success',
      buttons: false,
      timer: 3000,
      text: "<%= t('views.projects.created_msg') %>"}).then(function(){
      window.location = '<%= project_path(@project) %>'
  });
<% end %>
